algoritmo "somaNumeros"
    Procedimento somar()
        Var x: inteiro
            y: inteiro
            soma: inteiro
    Inicio
        escreval("Digite x:")
        leia(x)
        escreval("Digite y:")
        leia(y)
        soma <- x + y
        escreva(soma)
    fimprocedimento
Inicio
    somar()
fimalgoritmo